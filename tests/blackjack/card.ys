class Card
{
	var name = "null";
	var value = 0;

	func Card(self, name, value) {
		self.name = name;
		self.value = value;
	}

	func to_string(self) {
		var result = "{ ";
		result.append(self.name);
		result.append(", ");
		result.append(str(self.value));
		result.append(" }");
		return result;

		//return "{ " + self.name + ", " + str(self.value) + " }";
	}
}