// Contains io:: functions
import stdio;

func reverse(str) {
	io::println(str.reverse());
}

func main(args)
{
	io::println(args.length());
	io::println(args);
	io::println(args.get(0));
	
	var age = io::input("Enter your age: ");
	io::print("You are ");
	io::print(age);
	io::println(" years old!");

	var str = "Hello World";
	io::println(str);
	reverse(str);
}
